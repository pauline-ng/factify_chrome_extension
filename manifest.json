{
	"key": "",
   	"name": "Factify Chrome",
	"author": "Sun SAGONG",
	"version": "0.8.5",
	"manifest_version": 2,
	"description": "Factify Chrome Extension by using NativeMessaging",
	"icons": {
		"128": "icon.png"
	},

	"browser_action": {
    "default_icon": "icon.png",
		"default_popup": "popup.html"
  },

	"background": {
		"scripts": ["background.js"]
	},

	"permissions": [
		"tabs",
		"storage",
		"identity",
		"notifications",
		"http://*/*",
		"https://*/*",
		"nativeMessaging"
	],

	"web_accessible_resources": [
    	"icon.png"
  	],

	"options_page": "options.html"

}
